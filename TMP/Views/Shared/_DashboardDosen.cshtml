    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.4/Chart.min.js"></script>
    <script>
        // inisialisasi grafik pada saat halaman dimuat
        $(document).ready(function () {
            // Membuat grafik baru
            var ctx = document.getElementById("myChart").getContext('2d');
            var myChart = new Chart(ctx, {
                type: 'bar',
                data: {
                    labels: [],
                    datasets: [{
                        label: 'Persentase',
                        data: [],
                        backgroundColor: [], // Hapus warna latar belakang saat inisialisasi
                        borderColor: '',
                        borderWidth: 1
                    }]
                },
                options: {
                    legend: {
                        display: false
                    },
                    scales: {
                        yAxes: [{
                            ticks: {
                                beginAtZero: true,
                                min: 0, // Set nilai minimum sumbu Y menjadi 0
                                max: 100 // Set nilai maksimum sumbu Y menjadi 100
                            }
                        }]
                    }
                }
            });

            // Mengambil data dan label dari controller
            $.ajax({
                url: window.location.origin + '/Dashboard/getDataTahunDosen',
                type: 'GET',
                dataType: 'json',
                success: function (data) {
                    // Memperbarui data dan label pada grafik
                    myChart.data.labels = data.labels;
                    myChart.data.datasets[0].data = data.data;
                    // Memperbarui warna latar belakang berdasarkan persentase
                    myChart.data.datasets[0].backgroundColor = data.data.map(function (value) {
                        if (value > 50) {
                            return 'rgb(0, 128, 0)'; // Hijau
                        } else if (value >= 30) {
                            return 'rgb(255, 165, 0)'; // Kuning
                        } else {
                            return 'rgb(255, 0, 0)'; // Merah
                        }
                    });
                    myChart.update();
                },
                error: function () {
                    console.log("Gagal memuat data dari server.");
                }
            });

            // Fungsi untuk memperbarui grafik
            function updateChart(tahun) {
                // Mengirim permintaan ke fungsi di controller
                $.ajax({
                    url: window.location.origin + '/Dashboard/FilterChartDataTahunDosen',
                    type: 'POST',
                    data: { tahun: tahun },
                    dataType: 'json',
                    success: function (data) {
                        // Memperbarui data dan label pada grafik
                        myChart.data.labels = data.labels;
                        myChart.data.datasets[0].data = data.data;
                        // Memperbarui warna latar belakang berdasarkan persentase
                        myChart.data.datasets[0].backgroundColor = data.data.map(function (value) {
                            if (value > 50) {
                                return 'rgb(0, 128, 0)'; // Hijau
                            } else if (value >= 30) {
                                return 'rgb(255, 165, 0)'; // Kuning
                            } else {
                                return 'rgb(255, 0, 0)'; // Merah
                            }
                        });
                        myChart.update();
                    },
                    error: function () {
                        console.log("Gagal memuat data dari server.");
                    }
                });
            }

            // Fungsi untuk memperbarui grafik berdasarkan rentang tanggal
            function updateChartWithDateRangeDosen(tanggalAwal, tanggalAkhir) {
                // Mengirim permintaan ke fungsi di controller dengan data tanggalAwal dan tanggalAkhir
                $.ajax({
                    url: window.location.origin + '/Dashboard/FilterChartDataWithDateRangeDosen',
                    type: 'POST',
                    data: { tanggalAwal: tanggalAwal, tanggalAkhir: tanggalAkhir },
                    dataType: 'json',
                    success: function (data) {
                        // Memperbarui data dan label pada grafik
                        myChart.data.labels = data.labels;
                        myChart.data.datasets[0].data = data.data;
                        // Memperbarui warna latar belakang berdasarkan persentase
                        myChart.data.datasets[0].backgroundColor = data.data.map(function (value) {
                            if (value > 50) {
                                return 'rgb(0, 128, 0)'; // Hijau
                            } else if (value >= 30) {
                                return 'rgb(255, 165, 0)'; // Kuning
                            } else {
                                return 'rgb(255, 0, 0)'; // Merah
                            }
                        });
                        myChart.update();
                    },
                    error: function () {
                        alert("Gagal memuat data dari server.");
                    }
                });
            }


            // Menangani perubahan nilai pada dropdownlist
            $('#tahun').on('change', function () {
                var tahun = $(this).val();
                updateChart(tahun);
            });

            // Menangani perubahan nilai pada input date picker tanggal awal
            $('#tanggalAwal').on('change', function () {
                var tanggalAwal = $(this).val();
                var tanggalAkhir = $('#tanggalAkhir').val();
                updateChartWithDateRangeDosen(tanggalAwal, tanggalAkhir);
            });

            // Menangani perubahan nilai pada input date picker tanggal akhir
            $('#tanggalAkhir').on('change', function () {
                var tanggalAwal = $('#tanggalAwal').val();
                var tanggalAkhir = $(this).val();
                updateChartWithDateRangeDosen(tanggalAwal, tanggalAkhir);
            });
        });
    </script>


